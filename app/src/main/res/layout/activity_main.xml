<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Presenters.MainPresenter">

    <Button
        android:id="@+id/btnSchedule"
        android:layout_width="254dp"
        android:layout_height="62dp"
        android:layout_marginBottom="10dp"
        android:backgroundTint="#5C8AF2"
        android:text="CHILD SCHEDULE"
        app:layout_constraintBottom_toTopOf="@+id/btnPEF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent" />
    <Button
        android:id="@+id/btnPEF"
        android:layout_width="254dp"
        android:layout_height="62dp"
        android:layout_marginBottom="10dp"
        android:backgroundTint="#5C8AF2"
        android:text="PEF ENTRY"
        app:layout_constraintBottom_toTopOf="@+id/btnInventory"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/logout"
        android:layout_width="244dp"
        android:layout_height="63dp"
        android:backgroundTint="#5C8AF2"
        android:layout_marginBottom="24dp"
        android:text="SIGN OUT"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/checkin"
        android:layout_width="254dp"
        android:layout_height="62dp"
        android:backgroundTint="#5C8AF2"
        android:text="DAILY CHECK IN"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/logout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btnMedicineLogs"
        android:layout_width="254dp"
        android:layout_height="62dp"
        android:backgroundTint="#5C8AF2"
        android:text="LOG MEDICINE"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/checkin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btnInventory"
        android:layout_width="254dp"
        android:layout_height="62dp"
        android:backgroundTint="#5C8AF2"
        android:text="INVENTORY"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/btnMedicineLogs"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>